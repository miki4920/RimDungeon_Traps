<Defs>
	<ThingDef ParentName="BaseDungeonTrap">
		<defName>GasTrap</defName>
		<label>gas trap</label>
		<description>A Trap that can be filled with gas emiting chemicals.</description>
		<graphicData>
			<texPath>Things/Building/Traps/Gas_Trap</texPath>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsSecurity</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToBuild>50</WorkToBuild>
			<Flammability>1.0</Flammability>
			<TrapSpringChance>0</TrapSpringChance>
		</statBases>
		<building>
			<trapDestroyOnSpring>false</trapDestroyOnSpring>
			<trapUnarmedGraphicData>
				<texPath>Things/Building/Traps/Gas_Trap</texPath>
				<graphicClass>Graphic_Single</graphicClass>
			</trapUnarmedGraphicData>
			<fixedStorageSettings>
				<filter>
					<categories>
						<li>GasTanks</li>
					</categories>
				</filter>
			</fixedStorageSettings>
		</building>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>150</costStuffCount>
		<modExtensions>
			<li Class="RimDungeon.Trap_Def">
				<manualSpring>true</manualSpring>
				<rearmable>true</rearmable>
			</li>
		</modExtensions>
		<comps>
			<li Class="CompProperties_ChangeableProjectile"/>
			<li Class="RimDungeon.CompProperties_ArmedEmitter">
				<fleck>ArmedTrap</fleck>
				<emissionInterval>100</emissionInterval>
				<saveKeysPrefix>flash</saveKeysPrefix>
			</li>
		</comps>
		<inspectorTabs>
			<li>ITab_Shells</li>
		</inspectorTabs>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
		<researchPrerequisites>
			<li>GasTraps</li>
		</researchPrerequisites>
	</ThingDef>
</Defs>